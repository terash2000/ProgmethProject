package application;

public class Delay extends Thread {
	
	private boolean hold;
	
	public void run() {
		try {
			hold = true;
			Thread.sleep(1000);
			hold = false;
		} catch (InterruptedException e) {
			hold = false;
		}
	}

	public boolean isHold() {
		return hold;
	}

	public void setHold(boolean hold) {
		this.hold = hold;
	}
	
}